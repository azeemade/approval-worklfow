{
  "name": "azeem/approval-workflow",
  "description": "A flexible approval workflow package for Laravel applications.",
  "type": "library",
  "keywords": [
    "laravel",
    "approval",
    "workflow",
    "eloquent",
    "model"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Azeem",
      "email": "azeem@example.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0|^13.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Azeem\\ApprovalWorkflow\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Azeem\\ApprovalWorkflow\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Azeem\\ApprovalWorkflow\\ApprovalWorkflowServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
